{
  "name": "@mateothegreat/notion-sync",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "npm-run-all2 --parallel build:watch dev:run:export",
    "dev:run:export": "find . -not -path '**/node_modules/*' | entr -c bin/dev.js export",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "dev:dev": "vite",
    "prepack": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "devDependencies": {
    "@notionhq/client": "4.0.0",
    "@oclif/core": "^4.4.1",
    "@oclif/prettier-config": "0.2.1",
    "@oclif/test": "4.1.13",
    "@types/config": "^3.3.5",
    "@types/fast-levenshtein": "0.0.4",
    "@types/node": "^24.0.10",
    "@types/yaml": "^1.9.6",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "chalk": "5.4.1",
    "dotenv": "17.0.1",
    "npm-run-all2": "^8.0.4",
    "ora": "8.2.0",
    "ts-node": "10.9.2",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "^3.2.4"
  },
  "bin": {
    "notion-sync": "./bin/run.js"
  },
  "oclif": {
    "bin": "notion-sync",
    "commands": "./dist/commands",
    "dirname": "notion-sync",
    "helpClass": "./dist/lib/commands/help",
    "topicSeparator": " ",
    "theme": "./settings/cli-theme.json",
    "hooks": {
      "init": [
        "./dist/hooks/init"
      ],
      "preparse": [
        "./dist/hooks/preparse"
      ]
    },
    "plugins": [
      "@oclif/plugin-version",
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@inquirer/prompts": "7.6.0",
    "@oclif/plugin-version": "2.2.31",
    "ansis": "4.1.0",
    "fast-levenshtein": "3.0.0",
    "immer": "10.1.1",
    "rxjs": "7.8.2",
    "yaml": "^2.8.0",
    "zod": "^3.25.75",
    "zod-config": "^1.1.1"
  }
}
